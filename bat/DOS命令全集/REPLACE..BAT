@ECHO OFF

MKDIR TESTDIR
MKDIR TESTDIR\SUBTESTDIR
XCOPY /A /Y ATTRIB..BAT TESTFILE
ECHO > TESTDIR\TESTFILE
ECHO > TESTDIR\SUBTESTDIR\TESTFILE

DIR /S TESTFILE

REPLACE TESTFILE TESTDIR
REPLACE /A TESTFILE TESTDIR
REPLACE /R TESTFILE TESTDIR
REPLACE /U TESTFILE TESTDIR
REPLACE /S TESTFILE TESTDIR
DIR /S TESTFILE

DEL /Q TESTFILE
DEL /Q /S TESTDIR
RMDIR /Q /S TESTDIR
