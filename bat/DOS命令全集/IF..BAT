@ECHO OFF
IF /?

MKDIR TESTDIR
ECHO ZX> TESTFILE
(SET /P USER_TEST=) 0<TESTFILE

IF NOT ERRORLEVEL 1 ECHO ERRORLEVEL=%ERRORLEVEL%
IF "%USER_TEST%"=="ZX" ECHO IT IS ME.
IF EXIST TESTFILE DEL /Q TESTFILE
IF EXIST TESTDIR RMDIR /Q TESTDIR

IF "%USER_TEST%" EQU "ZX" ECHO IT IS ME.
IF DEFINED USER_TEST ECHO USER_TEST IS DEFINED.

DIR TEST*